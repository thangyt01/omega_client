    "framer-motion": "^11.0.24",
    "next": "^14.1.4",
    "next-intl": "^3.11.1",
    "next-useragent": "^2.8.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.1",
    "@chromatic-com/storybook": "1.2.26",
