    "stylelint-prettier": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn type-check"
    }
  },
  "license": "MIT",
  "simple-git-hooks": {
    "pre-commit": [
      "yarn lint-staged --allow-empty && yarn type-check"
    ]
  },
  "resolutions": {
    "string-width": "4.2.3",
    "jackspeak": "2.1.1"
  }
